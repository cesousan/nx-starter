{
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "affected": {
    "defaultBase": "main"
  },
  "npmScope": "hawk",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e", "build-storybook"]
      }
    }
  },
  "projects": {
    "ng-app": {
      "tags": [],
      "implicitDependencies": ["shared-assets", "shared-styles", "shared-ui-components"]
    },
    "ng-app-e2e": {
      "tags": [],
      "implicitDependencies": ["ng-app"]
    },
    "react-app": {
      "tags": [],
      "implicitDependencies": ["shared-assets", "shared-styles", "shared-ui-components"]
    },
    "react-app-e2e": {
      "tags": [],
      "implicitDependencies": ["react-app"]
    },
    "shared-assets": {
      "tags": ["scope:shared", "type:assets"]
    },
    "shared-styles": {
      "tags": ["scope:shared", "type:styles"]
    },
    "shared-environments": {
      "tags": ["scope:shared", "type:environments"]
    },
    "shared-data-access": {
      "tags": ["scope:shared", "type:data-access"]
    },
    "shared-ui-components": {
      "tags": ["scope:shared", "type:ui"]
    },
    "shared-ui-components-e2e": {
      "tags": [],
      "implicitDependencies": ["shared-ui-components"]
    },
    "shared-ui-components-angular": {
      "tags": ["scope:shared", "type:ui", "framework: angular"]
    },
    "shared-ui-components-react": {
      "tags": ["scope:shared", "type:ui", "framework: react"]
    },
    "shared-mappers": {
      "tags": ["scope:shared", "type:functions"]
    },
    "shared-types-front": {
      "tags": ["scope:shared", "type:types"]
    },
    "shared-types-api": {
      "tags": ["scope:shared", "type:types"]
    },
    "utils-dates": {
      "tags": ["scope:utils", "type:functions"]
    },
    "utils-observables": {
      "tags": ["scope:utils", "type:functions"]
    },
    "utils-arrays": {
      "tags": ["scope:utils", "type:functions"]
    },
    "utils-objects": {
      "tags": ["scope:utils", "type:functions"]
    },
    "utils-strings": {
      "tags": ["scope:utils", "type:functions"]
    },
    "utils-types-adt-remote-data": {
      "tags": ["scope:utils", "type:types"]
    },
    "utils-types-adt-maybe": {
      "tags": ["scope:utils", "type:types"]
    },
    "utils-types-adt-result": {
      "tags": ["scope:utils", "type:types"]
    },
    "utils-types-react": {
      "tags": ["scope:utils", "type:types"]
    }
  }
}
