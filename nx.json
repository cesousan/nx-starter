{
  "implicitDependencies": {
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    ".eslintrc.json": "*"
  },
  "affected": { "defaultBase": "main" },
  "npmScope": "cesousan",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "ng-app": {
      "tags": [],
      "implicitDependencies": ["shared-assets", "shared-styles"]
    },
    "ng-app-e2e": {
      "tags": [],
      "implicitDependencies": ["ng-app"]
    },
    "react-app": {
      "tags": [],
      "implicitDependencies": ["shared-assets", "shared-styles"]
    },
    "react-app-e2e": {
      "tags": [],
      "implicitDependencies": ["react-app"]
    },
    "shared-assets": {
      "tags": ["scope:shared", "type:assets"]
    },
    "shared-styles": {
      "tags": ["scope:shared", "type:styles"]
    },
    "shared-environments": {
      "tags": ["scope:shared", "type:environments"]
    },
    "shared-data-access": { "tags": ["scope:shared", "type:data-access"] },
    "utils-types": { "tags": ["scope:utils", "type:types"] }
  }
}
